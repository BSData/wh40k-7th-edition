    <profile id="03e9-468e-b65b-6110" profileTypeId="d5f97c0b-9fc9-478d-aa34-a7c414d3ea48" name="Astartes Grenade Launcher (Frag)" hidden="false" book="Codex Adeptus Astartes" page="191">

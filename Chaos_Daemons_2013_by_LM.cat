        <entry id="f558c8fe-10ed-5ff5-9b89-6ae97c7d1ca1" name="Psyker (Mastery Level 4)" points="0.0" categoryId="(No Category)" type="upgrade" minSelections="1" maxSelections="1" minPoints="0.0" maxPoints="-1.0" minInRoster="0" maxInRoster="-1" collective="false" page="0">

    <entry id="7369e767-a0fc-f023-3d7e-071f7026f670" name="The Blue Scribes" points="81.0" categoryId="485123232344415441232323" type="model" minSelections="0" maxSelections="1" minPoints="0.0" maxPoints="-1.0" minInRoster="0" maxInRoster="1" collective="false" page="0">

        <entry id="7e6c7fd5-45b7-e599-05cc-7d2d582c1252" name="The Staff of Cataclysm" points="0.0" categoryId="(No Category)" type="upgrade" minSelections="1" maxSelections="1" minPoints="0.0" maxPoints="-1.0" minInRoster="0" maxInRoster="-1" collective="false" page="0">

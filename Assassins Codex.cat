            <profile id="eb13-b971-b013-41de" name="Animus Speculum (Details)" book="Codex: Officio Assassinorum" hidden="false" profileTypeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" profileTypeName="Wargear Item">

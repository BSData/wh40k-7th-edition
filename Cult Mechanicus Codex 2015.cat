    <profile id="f51e-388a-8fdb-ab20" profileTypeId="d5f97c0b-9fc9-478d-aa34-a7c414d3ea48" name="Heavy Phosphor Blaster" hidden="false" book="Codex: Cult Mechanicus" page="75">

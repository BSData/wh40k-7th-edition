          <modifiers/>
        </rule>
        <rule id="3618f25c-2d4b-c0a1-e2d7-b83be04a8751" name="Ankh" hidden="false" page="0">
          <description>Whilst Ankh is on the table, models in this formation re-roll failed saving throws.</description>
          <modifiers/>
        </rule>
      </rules>
